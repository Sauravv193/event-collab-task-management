{
  "version": 2,
  "root": "frontend",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://event-collab-backend.onrender.com/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_VERSION": "20.18.0",
    "ROLLUP_USE_WASM": "1",
    "DISABLE_OPENCOLLECTIVE": "1",
    "ADBLOCK": "1",
    "CI": "true"
  }
}
